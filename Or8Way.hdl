/**
 * 8-way Or gate: 
 * out = in[0] Or in[1] Or ... Or in[7]
 */
CHIP Or8Way {
    IN in[8];
    OUT out;

    PARTS:
    Or(a=in[0], b=in[1], out=step1);
    Or(a=in[2], b=step1, out=step2);
    Or(a=in[3], b=step2, out=step3);
    Or(a=in[4], b=step3, out=step4);
    Or(a=in[5], b=step4, out=step5);
    Or(a=in[6], b=step5, out=step6);
    Or(a=in[7], b=step6, out=out);    
}